program scopeTest
{
    procedure KOO()
    {
        declare x;

        procedure joo()
        {
            print(x)
        }

        procedure poo()
        {
            call joo();
        }

        procedure foo()
        {
            call poo();
        }

        x := 999;
        call foo();
    }

    # main #
    call KOO();
}.
